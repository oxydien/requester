{
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devPath": "http://localhost:15523",
    "distDir": "../dist",
    "withGlobalTauri": false
  },
  "package": {
    "productName": "Requester",
    "version": "1.1.4"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": true
      },
      "os": {
        "all": true
      },
      "app": {
        "all": true
      }
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "oxy-requester",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ]
    },
    "security": {
      "csp": null
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "Requester",
        "width": 1000,
        "height": 520,
        "minWidth": 800,
        "minHeight": 350
      }
    ]
  }
}
